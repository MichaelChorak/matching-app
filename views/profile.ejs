<!doctype html>
<html lang="en">

<head>
    <!-- head -->
    <%- include('layout/head'); -%>
</head>

<body>
    <!-- navigation menu -->
    <main class="profilePage">
      
        <section>
            <h2>
                <%= title %>
            </h2>
        </section>

        <div class="container">
            <section>
                <img src="#" alt="afb">
                <!-- name-->
                <h2>
                
                </h2>
            </section>
            <% profileData.forEach(function(profile){ %>
            <section>
                <h2>Name: <%= profile.name %></h2>
                <h2>Age: <%= profile.age %>
                </h2>
                <h2>Gender: <%= profile.gender %>
                </h2>
                <h2>Favorite dish: <%= profile.favorite %>
                </h2>
            </section>

            <section>
                <h2>Settings</h2>
                <h2>Username:  <%= profile.name %>
                </h2>
                <h2>Password: </h2>
            </section>
        </div>
        <% }) %>
        <section>
            <button>Edit</button>
        </section>


        <section>
            <h2>Recept</h2>
        </section>

    </main>
    <!--footer-->
    <script src="scripts/script.js"></script>
</body>

</html>